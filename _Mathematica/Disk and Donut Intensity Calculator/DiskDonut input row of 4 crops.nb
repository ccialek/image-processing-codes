Cell[BoxData[
 RowBox[{
  RowBox[{"MyDiskDonutIntensity", "[", 
   RowBox[{
   "mycrops_", ",", "  ", "myDonutRadius_", ",", "  ", "myDiskRadius_", ",", 
    "  ", "myDiskWidth_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "myPadSize", ",", " ", "len", ",", " ", "centroidRaw", ",", 
      "centroidUltimate", ",", "centroid", " ", ",", " ", "masks", ",", "  ", 
